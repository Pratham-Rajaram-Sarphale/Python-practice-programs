   "source": [
    "#Write a program to check whether the string is palindrome or not\n",
    "word=input('Enter a word: ')\n",
    "lower=word.casefold()\n",
    "rev=lower[-1: :-1]\n",
    "if lower==rev:\n",
    "    print('This is a palindrome.')\n",
    "else:print('This is not a palindrome.')"
   ]
  }
